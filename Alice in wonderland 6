import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Program
{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int d1=n/10;
        int d2=n%10;
        int s=d1+d2;
        System.out.println("Bird said:" + n);
        System.out.println("Alice must go in path-" + s);
    }
}
