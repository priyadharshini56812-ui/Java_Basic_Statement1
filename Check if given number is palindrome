import java.util.Scanner;
class Solution {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        if(n < 100 || n > 999) {
            System.out.println("Invalid Input");
        } else if(n/100 == n%10) {
            System.out.println("palindrome");
        } else {
            System.out.println("not palindrome");
        }
    }
}
